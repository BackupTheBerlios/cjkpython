  !define MUI_UNICON "contrib\yi-box_uninstall.ico"
  !define MUI_CHECKBITMAP "contrib\yi-box_check.bmp"

  
;--------------------------------
;Languages

  !insertmacro MUI_LANGUAGE "English"
  !insertmacro MUI_LANGUAGE "Korean"
  !insertmacro MUI_LANGUAGE "SimpChinese"
  !insertmacro MUI_LANGUAGE "TradChinese"    
  !insertmacro MUI_LANGUAGE "Japanese"
 
  !insertmacro MUI_RESERVEFILE_LANGDLL

  !include "text-english.nsh"
  !include "text-korean.nsh"
  !include "text-japanese.nsh"
  !include "text-simpchinese.nsh"
  !include "text-tradchinese.nsh"
  

;--------------------------------
;Installation Types

InstType $(TypeFull)
InstType $(TypeFullJapanese)
InstType $(TypeBasic)
InstType $(TypeBasicJapanese)
InstType $(TypeMinimum)


;--------------------------------
;Installer Sections

ReserveFile "nsiscompileall.py"
Function .onInit
  ;Extract Install Options files
  ;$PLUGINSDIR will automatically be removed when the installer closes
  InitPluginsDir
  
  File "/oname=$PLUGINSDIR\nsiscompileall.py"  "nsiscompileall.py"

  # Set TypeFullJapanese by default on Japanese Systems
  IntCmp ${LANG_JAPANESE} $LANGUAGE set_japanese
  SetCurInstType 0
  Goto exit
set_japanese:
  SetCurInstType 1
exit:

FunctionEnd

